            </parameter>
		</command>

		<command name="export originals" code="Cap1EXPO" description="Perform an image export according to what is defined in the document's export originals settings.">
            <direct-parameter type="document"/>
			<parameter name="variants" code="COvl" description="The variants(s) to be exported">
				<type type="variant" list="yes"/>
			</parameter>
		</command>

        <command name="backup now" code="Cap1Bkup" description="Perform an immediate document backup (catalogs only). The default functionality is as defined in the user interface, but may be overridden using the below optional parameters.">
            <direct-parameter type="document"/>
            <parameter name="location" code="CBlo" optional="yes">
                <type type="text"/>
                <type type="file"/>
            </parameter>
            <parameter name="test integrity" code="CBti" type="boolean" optional="yes"/>
            <parameter name="optimize" code="CBop" type="boolean" optional="yes"/>
        </command>

		<command name="create template" code="Cap1Tmpl" description="Create a named template using this document. Will quietly overwrite an existing template of the same name.">
            <direct-parameter type="document"/>
			<parameter name="named" code="COTn" description="The name of the template to be created." type="text"/>
		</command>
		
		<command name="pack" code="Cap1PEIP" description="Pack an image into an Enhanced Image Package file.">
            <direct-parameter type="image"/>
		</command>

		<command name="unpack" code="Cap1UEIP" description="Unpack an image from an Enhanced Image Package file.">
            <direct-parameter type="image"/>
		</command>

        <command name="relink" code="Cap1Rlnk" description="Relink a catalog image file.  The file must be the exact same size and type as the originally imported image.">
            <direct-parameter type="image"/>
            <parameter name="to path" code="COpt" description="The new path for the image's file." type="text">
                <cocoa key="thePath"/>
            </parameter>
        </command>

		<command name="batch rename" code="Cap1BRnm" description="Perform a batch rename according to what is defined in the document's batch rename settings.">
            <direct-parameter type="document"/>
			<parameter name="variants" code="COvl" description="The variants(s) to be renamed">
				<type type="variant" list="yes"/>
			</parameter>
		</command>

		<command name="synchronize" code="Cap1Sync" description="Perform imports and/or deletes in a folder collection to reflect the contents of the actual folder on disk.  All parameters below are optional.">
            <direct-parameter type="collection"/>
			<parameter name="subfolders" code="COSs" description="Also consider the contents of all subfolders within the folder. Defaults to false." type="boolean" optional="yes"/>
			<parameter name="importing" code="COSi" description="Import any new images into the collection. Defaults to true." type="boolean" optional="yes"/>
			<parameter name="removing" code="COSr" description="Delete any variants in the collection for whom the files no longer exist. Defaults to false." type="boolean" optional="yes"/>
		</command>

		<command name="clear mask" code="Cap1Lclr" description="Clear the mask of a layer.">
            <direct-parameter type="layer"/>
		</command>

		<command name="invert mask" code="Cap1Linv" description="Invert the mask of a layer.">
            <direct-parameter type="layer"/>
		</command>

		<command name="fill mask" code="Cap1Lfil" description="Fill the mask of a layer.">
            <direct-parameter type="layer"/>
		</command>

		<command name="feather mask" code="Cap1Lfth" description="Feather the mask of a layer.">
            <direct-parameter type="layer"/>
			<parameter name="amount" code="COfa" type="real" description="The amount of feathering to perform, from 0 to 100."/>
		</command>

		<command name="copy mask" code="Cap1Lcop" description="Copy the mask of a layer.">
            <direct-parameter type="layer"/>
			<parameter name="to layer" code="COtl" type="layer" description="The layer to which the mask is to be copied."/>
		</command>

        <command name="apply style" code="Cap1Laps" description="Apply a style or preset to a layer.">
            <direct-parameter type="layer"/>
            <parameter name="named" code="COTn" type="text" description="The name of the style or preset to be applied."/>
        </command>

        <command name="clear luma range" code="Cap1LRcl" description="Clear the luma range from a layer.">
            <direct-parameter type="layer"/>
        </command>

		<command name="increment" code="Cap1CAin" description="Increment the value of the camera attribute.">
            <direct-parameter type="attribute"/>
		</command>

		<command name="decrement" code="Cap1CAde" description="Increment the value of the camera attribute.">
            <direct-parameter type="attribute"/>
		</command>
  
        <command name="adjust focus" code="Cap1Focs" description="Adjust the camera focus.">
            <direct-parameter type="camera"/>
            <parameter name="by amount" code="CFam" type="integer" description="The amount to adjust the focus, usually in a range between -3 to +3."/>
            <parameter name="sync" code="Sync" type="boolean" optional="yes" description="Should Capture One wait for the focus adjustment to complete before returning. Defaults to true."/>
        </command>

        <command name="apply workspace" code="Cap1LdWk" description="Load and apply a workspace.">
            <direct-parameter type="text" description="Name of the workspace to be applied."/>
        </command>

        <command name="silently quit" code="Cap1SiQt" description="Quit silently by automatically terminating any current activities (e.g. importing, exporting, Live View) and skipping any backup prompts that would normally appear."/>

        <command name="test mark" code="Cap1Ctmk" hidden="yes">
            <parameter name="named" code="COTn" type="text" optional="yes"/>
            <parameter name="time" code="Ctmt" type="boolean" optional="yes"/>
            <parameter name="memory" code="Ctmm" type="boolean" optional="yes"/>
            <parameter name="start time" code="Ctst" type="real" optional="yes"/>
        </command>

        <command name="test internal event" code="Cap1Ctie" hidden="yes">
            <parameter name="notification" code="Ctno" type="text"/>
        </command>

        <command name="test async event" code="Cap1Ctce" hidden="yes">
            <parameter name="named" code="COTn" type="text"/>
            <parameter name="notification" code="Ctno" type="text"/>
            <parameter name="next script path" code="Ctsp" type="text" optional="yes"/>
        </command>

        <enumeration name="select type" code="C1St">
            <enumerator name="next collection" code="SNCo"/>
            <enumerator name="previous collection" code="SPCo"/>
            <enumerator name="next set" code="SNSe"/>
            <enumerator name="previous set" code="SPSe"/>
            <enumerator name="next variant" code="SNVa"/>
            <enumerator name="previous variant" code="SPVa"/>
       </enumeration>

        <command name="change selection" code="Cap1CSel" description="Change the document's selection by the specified type.">
            <direct-parameter type="document"/>
            <parameter name="to" code="CtTy" type="select type" optional="no"/>
        </command>

        <command name="reset" code="Cap1Rset" description="Reset settings to defaults.">
            <direct-parameter type="item"/>
        </command>

    </suite>

</dictionary>
