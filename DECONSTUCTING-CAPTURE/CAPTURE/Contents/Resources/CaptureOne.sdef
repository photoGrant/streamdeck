<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionary SYSTEM "file://localhost/System/Library/DTDs/sdef.dtd">
<dictionary title="Dictionary">

	<suite name="Standard Suite" code="????" description="Common classes and commands for all applications.">
	
		<command name="open" code="aevtodoc" description="Open a document.">
			<direct-parameter description="The file(s) to be opened.">
				<type type="file" list="yes"/>
			</direct-parameter>
		</command>
		
		<command name="close" code="coreclos" description="Close a document.">
			<cocoa class="NSCloseCommand"/>
			<direct-parameter type="specifier" description="the document(s) or window(s) to close."/>
		</command>
		
		<command name="quit" code="aevtquit" description="Quit the application.">
			<cocoa class="NSQuitCommand"/>
		</command>
		
		<command name="count" code="corecnte" description="Return the number of elements of a particular class within an object.">
			<cocoa class="NSCountCommand"/>
			<direct-parameter type="specifier" description="The objects to be counted."/>
			<parameter name="each" code="kocl" type="type" optional="yes" description="The class of objects to be counted." hidden="yes">
				<cocoa key="ObjectClass"/>
			</parameter>
			<result type="integer" description="The count."/>
		</command>
		
		<command name="delete" code="coredelo" description="Delete an object.">
			<cocoa class="NSDeleteCommand"/>
			<direct-parameter type="specifier" description="The object(s) to delete."/>
		</command>

		<command name="exists" code="coredoex" description="Verify if an object exists."> 
			<cocoa class="NSExistsCommand"/> 
			<direct-parameter description="the object in question" type="specifier"/> 
			<result description="true if it exists, false if not" type="boolean"/> 
		</command> 
		
		<command name="make" code="corecrel" description="Create a new object.">
			<cocoa class="NSCreateCommand"/>
			<parameter name="new" code="kocl" type="type" description="The class of the new object.">
				<cocoa key="ObjectClass"/>
			</parameter>
			<parameter name="at" code="insh" type="location specifier" optional="yes" description="The location at which to insert the object.">
				<cocoa key="Location"/>
			</parameter>
			<parameter name="with data" code="data" type="any" optional="yes" description="The initial contents of the object.">
				<cocoa key="ObjectData"/>
			</parameter>
			<parameter name="with properties" code="prdt" type="record" optional="yes" description="The initial values for properties of the object.">
				<cocoa key="KeyDictionary"/>
			</parameter>
			<result type="specifier" description="The new object."/>
		</command>

		<command name="move" code="coremove" description="Move an object to a new location.">
			<cocoa class="POMoveCommand"/>
			<direct-parameter type="specifier" description="The object(s) to move."/>
			<parameter name="to" code="insh" type="location specifier" description="The new location for the object(s).">
				<cocoa key="ToLocation"/>
			</parameter>
		</command>

		<class name="application" code="capp" description="The application's top-level scripting object.">
			<cocoa class="NSApplication"/>
			<property name="name" code="pnam" type="text" access="r" description="The name of the application."/>
			<property name="frontmost" code="pisf" type="boolean" access="r" description="Is this the active application?">
				<cocoa key="isActive"/>
			</property>
			<property name="version" code="vers" type="text" access="r" description="The version number of the application."/>
			<element type="window" access="r">
				<cocoa key="orderedWindows"/>
			</element>
			<responds-to name="quit">
				<cocoa method="handleQuitScriptCommand:"/>
			</responds-to>
		</class>

		<class name="window" code="cwin" description="A window.">
			<cocoa class="NSWindow"/>
			<property name="name" code="pnam" type="text" access="r" description="The title of the window.">
				<cocoa key="title"/>
			</property>
			<property name="id" code="ID  " type="integer" access="r" description="The unique identifier of the window.">
				<cocoa key="uniqueID"/>
			</property>
			<property name="index" code="pidx" type="integer" description="The index of the window, ordered front to back.">
				<cocoa key="orderedIndex"/>
			</property>
			<property name="bounds" code="pbnd" type="rectangle" description="The bounding rectangle of the window.">
				<cocoa key="boundsAsQDRect"/>
			</property>
			<property name="closeable" code="hclb" type="boolean" access="r" description="Does the window have a close button?">
				<cocoa key="hasCloseBox"/>
			</property>
			<property name="miniaturizable" code="ismn" type="boolean" access="r" description="Does the window have a minimize button?">
				<cocoa key="isMiniaturizable"/>
			</property>
			<property name="miniaturized" code="pmnd" type="boolean" description="Is the window minimized right now?">
				<cocoa key="isMiniaturized"/>
			</property>
			<property name="resizable" code="prsz" type="boolean" access="r" description="Can the window be resized?">
				<cocoa key="isResizable"/>
			</property>
			<property name="visible" code="pvis" type="boolean" description="Is the window visible right now?">
				<cocoa key="isVisible"/>
			</property>
			<property name="zoomable" code="iszm" type="boolean" access="r" description="Does the window have a zoom button?">
				<cocoa key="isZoomable"/>
			</property>
			<property name="zoomed" code="pzum" type="boolean" description="Is the window zoomed right now?">
				<cocoa key="isZoomed"/>
			</property>

			<property name="document" code="docu" type="document" access="r" description="The document whose contents are displayed in the window."/>
			
			<responds-to name="close">
				<cocoa method="handleCloseScriptCommand:"/>
			</responds-to>
		</class>
		
	</suite>

	<suite name="Capture One Suite" code="Cap1" description="Common classes and commands for Capture One.">
		<value-type name="RGB color" code="cRGB" description="Color defined by red, green, and blue values (each 0-65535).">
			<cocoa class="NSColor"/>
		</value-type>
		<enumeration name="nextcaptureadjustments" code="NCAd">
			<cocoa name="NextCaptureAdjustments"/>
			<enumerator name="Default" code="NCda">
				<cocoa name="DefaultAdjustments"/>
			</enumerator>
			<enumerator name="Last" code="NCla">
				<cocoa name="LastAdjustments"/>
			</enumerator>
			<enumerator name="Primary" code="NCpa">
				<cocoa name="PrimaryAdjustments"/>
			</enumerator>
			<enumerator name="Using Clipboard" code="NCuc">
				<cocoa name="UsingClipboardAdjustments"/>
			</enumerator>
			<enumerator hidden="yes" name="Last With Variants" code="Nlwv">
				<cocoa name="LastWithVariantsAdjustments"/>
			</enumerator>
			<enumerator hidden="yes" name="Primary With Variants" code="Npwv">
				<cocoa name="PrimaryWithVariantsAdjustments"/>
			</enumerator>
		</enumeration>
		<enumeration name="shutterlatency" code="ShLa">
			<cocoa name="ShutterLatency"/>
			<enumerator name="Normal Latency" code="SLNo" description="Normal(Long) Shutter Latency">
				<cocoa name="NormalShutterLatency"/>
			</enumerator>
			<enumerator name="Zero Latency" code="SLZe" description="Zero(Short) Shutter Latency">
				<cocoa name="ZeroShutterLatency"/>
			</enumerator>
			<enumerator name="Unknown Latency" code="SLUn" description="Unknown Shutter Latency - may not be supported">
				<cocoa name="UnknownShutterLatency"/>
			</enumerator>
		</enumeration>

		<class name="application" code="capp" description="Capture One application class.  Directly contained image and variant elements are for the current collection of the current document." inherits="application">
			<cocoa class="Application"/>

			<element type="document">
				<cocoa key="scriptDocuments"/>
			</element>
			
			<element type="image" access="r">
				<cocoa key="images"/>
				<accessor style="test"/>
				<accessor style="relative"/>
				<accessor style="range"/>
				<accessor style="name"/>
				<accessor style="id"/>
				<accessor style="index"/>
			</element>
            
            <element type="variant" access="r"/>
			
			<property name="current document" code="Cudo" description="(PRO Only) The currently frontmost document." type="document"/>

            <property name="viewer" code="COVw" type="viewer configuration" description="The Viewer window's viewer settings."/>

            <property name="progress total units" code="PrTo" type="integer" description="Set this property to the total number of units of work your script will be performing. While this property is greater than the 'progress completed units' property, Capture One will display a progress window."/>
            <property name="progress completed units" code="PrCo" type="integer" description="Set this property to the number of units of work your script has performed."/>
            <property name="progress text" code="PrDe" type="text" description="Set this to a simple explanation of the work being performed. (optional)"/>
            <property name="progress additional text" code="PrDa" type="text" description="Set this to additional specifics of the work being performed. (optional)"/>

			<property name="capture done script" code="CDas" description="(PRO Only) The AppleScript that gets called when an image has completed being captured.">
				<type type="text"/>
				<type type="file"/>
			</property>
			<property name="processing done script" code="Eops" description="(PRO Only) The AppleScript that gets called when a file is finished processing. The script is passed the identifier string of the finished batch job in argv 1, the original RAW file path in argv 2, and a list of the output file paths in argv 3.">
				<type type="text"/>
				<type type="file"/>
			</property>
			<property name="batch done script" code="BDas" description="(PRO Only) The AppleScript that gets called when the batch process queue completes.">
				<type type="text"/>
				<type type="file"/>
			</property>
			<property name="live view became ready script" code="LVrd" description="(PRO Only) The AppleScript that gets called when Live View has become ready.">
				<type type="text"/>
				<type type="file"/>
			</property>
			<property name="live view will close script" code="LVwc" description="(PRO Only) The AppleScript that gets called when Live View is about to close.">
				<type type="text"/>
				<type type="file"/>
			</property>
			<property name="live view done script" code="LVas" description="(PRO Only) The AppleScript that gets called when Live View is closed.">
				<type type="text"/>
				<type type="file"/>
			</property>
            <property name="barcode scanned script" code="BCsc" description="(Enterprise Only) The AppleScript that gets called when a barcode has been scanned.">
                <type type="text"/>
                <type type="file"/>
            </property>
			<property name="selected variants" code="csev" description="List of selected variants in the front document's browser (deprecated)." access="r" hidden="yes">
				<type type="variant" list="yes"/>
			</property>
			<property name="all variants" code="calv" description="(PRO Only) List of all variants in the front document's browser (deprecated)." access="r" hidden="yes">
				<type type="variant" list="yes"/>
			</property>
			<property name="app version" code="cver" description="The version of Capture One" type="text" access="r"/>
			<property name="primary variant" code="cprv" description="The primary selected variant" type="variant" access="r"/>
			<property name="next capture adjustments" code="cnca" description="(PRO Only) When an image is captured, what adjustments should be applied? (deprecated)" type="nextcaptureadjustments" hidden="yes"/>
			<property name="shutter latency" code="cshl" description="(PRO Only) Choose the Shutter Latency Mode used when capturing. Only has an effect when a camera is connected." type="shutterlatency"/>
            <property name="display acceleration" code="Cocd" type="openCLStatus" description="When should hardware acceleration be used for display.">
                <cocoa key="openCLDisplay"/>
			</property>
            <property name="processing acceleration" code="Copr" type="openCLStatus" description="When should hardware acceleration be used for processing.">
                <cocoa key="openCLProcessing"/>
			</property>
			<responds-to name="open">
				<cocoa method="handleOpenScriptCommand:"/>
			</responds-to>
			<responds-to name="begin live view">
				<cocoa method="handleBeginLiveViewScriptCommand:"/>
			</responds-to>
			<responds-to name="end live view">
				<cocoa method="handleEndLiveViewScriptCommand:"/>
			</responds-to>
            <responds-to name="apply workspace">
                <cocoa method="handleApplyWorkspaceCommand:"/>
            </responds-to>
            <responds-to name="silently quit">
                <cocoa method="handleSilentQuitScriptCommand:"/>
            </responds-to>
            <property name="test name" code="Ctnm" type="text" hidden="yes"/>
            <responds-to name="test mark" hidden="yes">
                <cocoa method="handleTestMarkCommand:"/>
            </responds-to>
            <responds-to name="test internal event" hidden="yes">
                <cocoa method="handleTestInternalEventCommand:"/>
            </responds-to>
            <responds-to name="test async event" hidden="yes">
                <cocoa method="handleTestAsyncEventCommand:"/>
            </responds-to>
		</class>


		<enumeration name="documentType" code="COdt"> 
			<enumerator name="session" code="COsd" description="Session document."/> 
			<enumerator name="catalog" code="COct" description="Catalog document."/> 
		</enumeration> 

		<class name="document" code="docu" description="A Capture One document.  Directly contained image and variant elements are for the current collection.">
			<cocoa class="DocumentAppleScript"/>
			<property name="name" code="pnam" type="text" access="r" description="The name of the document.">
				<cocoa key="displayName"/>
			</property>
            <property name="id" code="ID  " type="text" access="r" description="The unique identifier of the document.">
                <cocoa key="uuid"/>
            </property>
			<element description="The images in the current collection" type="image" access="r">
				<cocoa key="images"/>
				<accessor style="test"/>
				<accessor style="relative"/>
				<accessor style="range"/>
				<accessor style="name"/>
				<accessor style="index"/>
			</element>
			<element type="recipe">
				<cocoa key="scriptRecipes"/>
			</element>
			<element type="job">
				<cocoa key="scriptJobs"/>
			</element>
			<element type="collection"/>
			<element type="variant"/>
			<element type="keyword"/>
            <element type="tool tab"/>
            <element type="toolbar item"/>
            <property name="window toolbar" code="CVwT" type="boolean"/>
			<property name="current collection" code="Cuco" description="The current collection." type="collection"/>
            <property name="current recipe" code="Cure" description="The currently selected recipe." type="recipe"/>
			<property name="template" code="COtm" access="r" description="The template name to use (during 'make new' only)" type="text">
				<cocoa key="templateName"/>
			</property>
			<property name="path" code="ipth" access="r" description="The folder item containing the document.">
				<type type="text"/>
				<type type="file"/>
				<cocoa key="rootFolder"/>
			</property>
			<property name="folder" code="COdf" access="r" type="file" description="The folder item of the document."/>
			<property name="kind" code="COdk" type="documentType" access="r" description="The kind of document.">
				<cocoa key="documentType"/>
			</property>
            <property name="filters" code="COft" description="The current filters being used.  Set to an empty list {} to clear the filters.">
 				<type type="text" list="yes"/>
			</property>
           <property name="available filters" code="COaf" access="r" description="The filters currently available to be used in this document.">
				<type type="text" list="yes"/>
			</property>
			<property name="output" code="COof" access="rw" description="The Output folder item.">
				<type type="text"/>
				<type type="file"/>
				<cocoa key="outputFolder"/>
			</property>
			<property name="output sub path" code="COos" type="text" access="rw" description="The Output sub folder item, with tokens in brackets e.g. [Rating].">
				<cocoa key="outputSubFolder"/>
			</property>
			<property name="output counter" code="COOc" type="integer" access="rw"/>
			<property name="output counter increment" code="COOi" type="integer" access="rw"/>
			<property name="captures" code="COcf" access="rw" description="The Capture folder item.">
				<type type="text"/>
				<type type="file"/>
				<cocoa key="capturesFolder"/>
			</property>
			<property name="selects" code="COsf" access="rw" description="The Selects folder item (Session only).">
				<type type="text"/>
				<type type="file"/>
				<cocoa key="selectsFolder"/>
			</property>
			<property name="trash" code="COtf" access="rw" description="The Trash folder item (Session only).">
				<type type="text"/>
				<type type="file"/>
				<cocoa key="trashFolder"/>
			</property>
            <property name="barcode" code="CObc" type="text" description="(Enterprise Only) The current scanned barcode value." access="rw"/>
			<property name="all variants" code="Cdav" description="List of all variants in the current collection (deprecated)." access="r" hidden="yes">
				<type type="variant" list="yes"/>
			</property>
			<responds-to name="browse">
				<cocoa method="handleBrowseScriptCommand:"/>
			</responds-to>
			<responds-to name="close">
				<cocoa method="handleCloseScriptCommand:"/>
			</responds-to>
			<responds-to name="refresh" hidden="yes">
				<cocoa method="handleRefreshScriptCommand:"/>
			</responds-to>
			<property name="processing queue enabled" code="COQP" type="boolean" access="rw" description="Whether or not the output processing batch job queue is running.  Can be changed.">
				<cocoa key="processQueueEnabled"/>
			</property>
            <property name="document window" code="dWin" type="window" description="The document's associated window."/>
            <property name="tool settings" code="COTl" type="tools configuration" description="The document's tool settings.  See also the document's “tool tab” elements for specific tab and tool configuration."/>
            <property name="viewer" code="COVw" type="viewer configuration" description="The document's viewer settings.  Note that any changes to one document's viewer will affect all document viewers."/>
            <property name="browser" code="COBr" type="browser configuration" description="The document's browser settings."/>
            <property name="output name format" code="COOF" type="text" access="rw" description="The current process output naming format, with tokens in brackets e.g. [Rating]."/>
			<property name="output name" code="COjn" type="text" access="rw" description="The current process output job name."/>
			<property name="import counter" code="COic" type="integer" access="rw" description="The current import counter.  The next item imported will be incremented from this." hidden="yes"/>
			<property name="capture counter" code="COCc" type="integer" access="rw"/>
			<property name="capture counter increment" code="COCi" type="integer" access="rw"/>
			<property name="capture name format" code="COCF" type="text" access="rw" description="The current next capture naming format, with tokens in brackets e.g. [Rating]."/>
			<property name="capture name" code="COCn" type="text" access="rw" description="The current next capture name."/>
			<property name="last captured file" code="COCp" type="file" access="r" description="The last captured image file."/>
            <property name="normalize target color" code="COnc" type="RGB color" access="rw" description="The target color for applying normalizations, using the Generic RGB profile."/>
            <property name="camera" code="COco" type="camera" access="r" description="The selected camera."/>
            <property name="available camera identifiers" code="CAid" access="r" description="The identifiers of all tethered cameras, for use with the 'select camera' command.">
                <type type="text" list="yes"/>
            </property>
            <property name="workspace lock PIN" code="COlp" type="text" access="w" description="(Enterprise Only)">
                <cocoa key="workspaceLockPassword"/>
            </property>
            <property name="copy stand" code="COcs" type="control device" access="r" description="(CH Only) The attached column control copy stand device."/>
			<property name="batch rename settings" code="Cbrs" description="The document's batch rename settings." type="batch rename options"/>
			<property name="import settings" code="Cimp" type="import options" description="The document's import settings."/>
			<property name="export original settings" code="Cexp" type="export original options" description="The document's export original settings."/>
            <property name="next capture settings" code="Cncs" description="What settings and adjustments are applied when an image is captured from a tethered camera." type="next capture options"/>
            <property name="overlay settings" code="COst" description="The document's overlay settings." type="overlay options"/>
            <property name="server settings" code="Ccps" type="server options" description="The document's Capture Pilot server settings."/>
            <property name="grid settings" code="COgr" type="grid options" description="The document's grid settings."/>
            <property name="guide settings" code="COgs" type="guide options" description="(Enterprise Only) The document's guide settings."/>
			<responds-to name="select camera">
				<cocoa method="handleChooseCameraScriptCommand:"/>
			</responds-to>
			<responds-to command="select">
				<cocoa method="handleSelectCommand:"/>
			</responds-to>
			<responds-to command="deselect">
				<cocoa method="handleDeselectCommand:"/>
			</responds-to>
            <responds-to command="change selection">
                <cocoa method="handleChangeSelectCommand:"/>
            </responds-to>
			<responds-to command="batch rename">
				<cocoa method="handleBatchRenameCommand:"/>
			</responds-to>
			<responds-to command="import">
				<cocoa method="handleImportCommand:"/>
			</responds-to>
			<responds-to command="export originals">
				<cocoa method="handleExportOriginalsCommand:"/>
			</responds-to>
            <responds-to command="backup now">
                <cocoa method="handleBackupCommand:"/>
            </responds-to>
			<responds-to command="create template">
				<cocoa method="handleCreateTemplateCommand:"/>
			</responds-to>
		</class>

        <class name="toolbar item" code="COti" description="A Capture One window toolbar item.">
            <cocoa class="ToolbarItemAppleScript"/>
            <property name="name" code="pnam" type="text" access="r"/>
            <property name="id" code="ID  " type="text" access="r" description="The unique identifier of the item.">
                <cocoa key="uuid"/>
            </property>
        </class>

        <class name="tool tab" code="COtt" description="A Capture One tool tab. Newly created tool tabs must have either a standard id, or a name and valid icon number (1-18).">
            <cocoa class="InspectorToolTabAppleScript"/>
            <element type="tool"/>
            <property name="name" code="pnam" type="text" access="r"/>
            <property name="id" code="ID  " type="text" access="r" description="The unique identifier of the tab.">
                <cocoa key="uuid"/>
            </property>
            <property name="icon number" code="CTin" type="integer" access="r"/>
            <property name="selected" code="vsel" description="Is this tool tab selected in the UI." type="boolean" access="rw"/>
        </class>

        <class name="tool" code="COto" description="A Capture One tool.">
            <cocoa class="InspectorToolAppleScript"/>
            <property name="name" code="pnam" type="text" access="r"/>
            <property name="id" code="ID  " type="text" access="r" description="The unique identifier of the tool.">
                <cocoa key="uuid"/>
            </property>
            <property name="expanded" code="CTex" description="Is this tool expanded in the UI." type="boolean" access="rw"/>
            <property name="locked" code="COlk" description="(Enterprise Only) Is this tool locked." type="boolean" access="rw"/>
        </class>
        
        <enumeration name="focus indication" code="Cfce" hidden="yes">
            <enumerator name="no indicator" code="Cfno"/>
            <enumerator name="focused" code="Cffo"/>
            <enumerator name="unfocused" code="Cfun"/>
            <enumerator name="too near" code="Cftn"/>
            <enumerator name="too far" code="Cftf"/>
            <enumerator name="front focus" code="Cfff"/>
            <enumerator name="behind focus" code="Cfbf"/>
            <enumerator name="driving" code="Cfdr"/>
            <enumerator name="tracking" code="Cftr"/>
            <enumerator name="unknown" code="Cfuk" description="No information, not possible, error, or other unknown indicator status."/>
        </enumeration>

		<class name="camera" code="CObj" description="A camera. Not all properties are supported by all cameras.">
			<cocoa class="CameraAppleScript"/>
            <element type="attribute"/>
            <element type="focus meter"/>
			<property name="name" code="pnam" type="text" access="r" description="The name of the camera.">
				<cocoa key="displayName"/>
			</property>
            <property name="identifier" code="CCid" type="text" access="r" description="The camera's unique identifier.">
                <cocoa key="uuid"/>
            </property>
			<property name="format" code="CCfm" type="text" access="rw" description="Current image format."/>
			<property name="sensor plus" code="CCsp" type="text" access="rw" description="Current Sensor+ setting."/>
			<property name="image area" code="CCar" type="text" access="rw" description="Current image area format."/>
            <property name="live width" code="CCcw" type="integer" access="r" description="Current live view capture width in pixels."/>
            <property name="live height" code="CCch" type="integer" access="r" description="Current live view capture height in pixels."/>
			<property name="ISO" code="CCis" type="text" access="rw" description="Current ISO setting."/>
			<property name="white balance" code="CCwb" type="text" access="rw" description="Current white balance setting.">
				<cocoa key="balance"/>
			</property>
			<property name="program" code="CCpr" type="text" access="rw" description="Current exposure program mode."/>
			<property name="exposure step" code="CCst" type="text" access="rw" description="Current exposure step.">
